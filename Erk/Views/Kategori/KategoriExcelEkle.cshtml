@{
    ViewData["Title"] = "KategoriExcelEkle";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<form asp-action="KategoriExcelEkle" enctype="multipart/form-data" method="post">
    <div class="form-group">
        <label for="excelFile">Excel Dosyası:</label>
        <input type="file" name="excelFile" class="form-control" required />
    </div>
    <button type="submit" class="btn btn-primary">Yükle ve Ekle</button>
</form>